{
	"name": "wholescreen.js",
	"version": "0.1.0",
	"description": "Minimal cross-browser wrapper of the Fullscreen API. Handles vendor prefixes for you!",
	"author": "Albin Larsson",
	"license": "MIT",
	"repository": "git+https://github.com/friday/wholescreen.git",
	"main": "dist/wholescreen.js",
	"types": "dist/wholescreen.d.ts",
	"scripts": {
		"build": "tsc && rollup --config; mv .js/wholescreen.d.ts dist && rimraf .js",
		"lint": "tsc --noEmit && tslint --project tsconfig.json --format stylish",
		"prebuild": "rimraf dist",
		"preversion": "npm build",
		"postversion": "git push && git push --tags"
	},
	"keywords": [
		"fullscreen",
		"browser",
		"prefixes",
		"whole",
		"screen",
		"typescript"
	],
	"files": [
		"dist"
	],
	"devDependencies": {
		"rimraf": "^2.6.1",
		"rollup": "^0.62.0",
		"rollup-plugin-terser": "^1.0.1",
		"tslint": "^5.10.0",
		"typescript": "^2.9.0"
	}
}
